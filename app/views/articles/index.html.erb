<div class="banner">
<a href="/articles">WikiPierrick</a>
<a href="/articles/new">New article</a>
</div>

<ul style="list-style: none;">
  <% @articles.each do |article| %>
    <li class="articles">
      <h2><%= link_to article.title, article_path(article) %></h2>
       <p><%= article.content %></p>
    </li>
  <% end %>
</ul>
